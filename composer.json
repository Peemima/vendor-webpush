{
    "name": "peemima/vendor-webpush",
    "description": "A project using WebPush",
    "require": {
        "php": "^8.0",
        "google/web-push": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
